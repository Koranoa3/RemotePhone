{
    "theme": {
        "base_theme": "dark"
    },
    "button": {
        "holdTime": 300,
        "holdClickInterval": 50
    },
    "trackpad": {
        "sensitivity": 1.0,
        "acceleration": 1.6,
        "scrollSpeed": 3.0,
        "doubleClickSpeed": 200
    },
    "layout": [
        [
            {
                "label": "Volume",
                "type": "slider",
                "command": "volume_change",
                "multiplier": 0.2,
                "horizontal": false,
                "tap": "mute",
                "position": {
                    "x": 3,
                    "y": 2
                },
                "size": {
                    "width": 1,
                    "height": 2
                }
            },
            {
                "label": "\u25b2",
                "type": "action",
                "command": "up",
                "position": {
                    "x": 2,
                    "y": 3
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "\u25bc",
                "type": "action",
                "command": "down",
                "position": {
                    "x": 2,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "\u25c0",
                "type": "action",
                "command": "left",
                "position": {
                    "x": 1,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "\u25b6",
                "type": "action",
                "command": "right",
                "position": {
                    "x": 3,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Play/Pause",
                "type": "action",
                "command": "play_pause",
                "position": {
                    "x": 2,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "PrevTrack",
                "type": "action",
                "command": "prev_track",
                "position": {
                    "x": 1,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "NextTrack",
                "type": "action",
                "command": "next_track",
                "position": {
                    "x": 3,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            }
        ],
        [
            {
                "label": "Virtual Desktop",
                "type": "slideaction",
                "command": {
                    "type": "action",
                    "positive": "next_virtual_desktop",
                    "negative": "prev_virtual_desktop",
                    "tap": [
                        "win",
                        "tab"
                    ]
                },
                "threshold": 60,
                "horizontal": true,
                "position": {
                    "x": 1,
                    "y": 1
                },
                "size": {
                    "width": 2,
                    "height": 1
                }
            },
            {
                "label": "Task View",
                "type": "action",
                "command": [
                    "win",
                    "tab"
                ],
                "position": {
                    "x": 3,
                    "y": 1
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Browser Tab",
                "type": "slideaction",
                "command": {
                    "type": "action",
                    "positive": [
                        "ctrl",
                        "tab"
                    ],
                    "negative": [
                        "shift",
                        "ctrl",
                        "tab"
                    ]
                },
                "threshold": 30,
                "horizontal": true,
                "position": {
                    "x": 1,
                    "y": 3
                },
                "size": {
                    "width": 3,
                    "height": 1
                }
            },
            {
                "label": "Back",
                "type": "action",
                "command": [
                    "alt",
                    "left"
                ],
                "position": {
                    "x": 1,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Forward",
                "type": "action",
                "command": [
                    "alt",
                    "right"
                ],
                "position": {
                    "x": 3,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            {
                "label": "Mid Click",
                "type": "button",
                "command": "middle",
                "position": {
                    "x": 2,
                    "y": 4
                },
                "size": {
                    "width": 1,
                    "height": 1
                }
            }
        ]
    ],
    "prefered_layout_mode": "container trackpad"
}